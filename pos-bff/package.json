{
  "name": "pos-bff",
  "version": "0.0.1",
  "description": "pos-bff",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run lint && nest start",
    "start:dev": "npm run lint && nest start --watch",
    "start:debug": "npm run lint &&  nest start --debug --watch",
    "start:prod": "npm run lint && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build:tsc": "tsc && tsc-alias",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-parameter-properties": 0,
      "@typescript-eslint/interface-name-prefix": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/ban-types": "off",
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/swagger": "^7.3.1",
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.8",
    "cache-manager": "^5.5.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.9",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "express-ejs-layouts": "^2.5.1",
    "form-data": "^4.0.0",
    "helmet": "^5.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mariadb": "^3.0.1",
    "moment": "^2.29.4",
    "nest-winston": "^1.9.4",
    "newrelic": "^11.16.0",
    "pkce-challenge": "^2.2.0",
    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/express-ejs-layouts": "^2.5.0",
    "@types/hpp": "^0.2.2",
    "@types/jest": "28.1.x",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.1",
    "@types/multer": "^1.4.11",
    "@types/newrelic": "^9.14.3",
    "@types/node": "^18.0.4",
    "@types/supertest": "^2.0.12",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "28.1.x",
    "lint-staged": "^13.0.3",
    "node-json-transform": "^1.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.4.5"
  }
}
